    "test": "node scripts/test.js --env=jsdom",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "lint": "tslint -c tslint.json --project tsconfig.json"
  },
  "devDependencies": {
    "@types/react": "16.0.34",
    "@types/react-dom": "16.0.3",
    "@types/react-redux": "5.0.14",
